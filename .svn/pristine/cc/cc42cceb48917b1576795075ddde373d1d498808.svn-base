/*扁平化效果*/
input.form-control, input.btn, button.btn, a.btn, select.form-control, textarea.form-control{
	border-radius: 0;
}

/*分页*/
div.pagination, ul.pagination{
	margin: 0;
}

.input-append{
    margin-bottom: 0;
    vertical-align: middle;
    font-size: 0;
    white-space: nowrap;
}

/*公共表单样式*/
