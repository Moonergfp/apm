   .content-main-edit .element.active .ui-resizable-n,  .content-main-edit .element.active .ui-resizable-s{
   height: 0px; 
   border-bottom: 1px solid #08a1ef;
 }
   /* .content-main-edit .ui-resizable-n{
     top: 0px;
   }
