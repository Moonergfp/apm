  .content-main-edit .element.active .ui-resizable-e,  .content-main-edit .element.active .ui-resizable-w{
	width: 0px; 
   border-right: 1px solid #08a1ef;
 }
  .content-main-edit .element.active .ui-resizable-ne,  .content-main-edit .element.active .ui-resizable-se,  .content-main-edit .element.active .ui-resizable-sw,  .content-main-edit .element.active .ui-resizable-nw{
  border: 1px solid #08a1ef;
  background-color: #fff;
 }
 
  .content-main-edit .element.active .ui-resizable-se{
  width: 9px;
  height: 9px;
  right: -5px;
  bottom: -5px;
 }
 
   .content-main-edit .element.active .ui-resizable-n:after,.content-main-edit .element.active .ui-resizable-s:after,.content-main-edit .element.active .ui-resizable-e:after,.content-main-edit .element.active .ui-resizable-w:after{
   content:"";
   display:block;
   width:9px;
   height:9px;
   border:1px solid #08a1ef;
   background-color: #fff;
   position:absolute;
   
   }
.content-main-edit .element.active .ui-resizable-n:after,.content-main-edit .element.active .ui-resizable-s:after{
	top:-4px;
   left:50%;
   margin-left:-4px;
}

.content-main-edit .element.active .ui-resizable-w:after,.content-main-edit .element.active .ui-resizable-e:after{
	top:50%;
   left:-4px;
   margin-top:-4px;
}

 .content-main-edit .element.active .ui-resizable-ne{
       right: -9px;
  	top: -9px;
   }
   .content-main-edit .element.active .ui-resizable-se{
       right: -9px;
  		bottom: -9px;
   }
   .content-main-edit .element.active .ui-resizable-sw {
	  left: -9px;
